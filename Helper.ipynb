   "source": [
    "## To get the transpose of a matrix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "152d2543",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[[1, 6, 11], [2, 7, 12], [3, 8, 13]]"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "matrix = [[1,2,3],[6,7,8],[11,12,13]]\n",
    "for i in range(len(matrix)):\n",
    "        for j in range(i+1, len(matrix)):\n",
    "            matrix[i][j], matrix[j][i] = matrix[j][i], matrix[i][j]\n",
    "matrix"
   ]
  }
